    "url": "https://github.com/tokyo-lab/dirtybird/issues"
  },
  "homepage": "https://github.com/tokyo-lab/dirtybird#readme",
  "devDependencies": {
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "parcel": "^2.5.0",
    "prettier": "^2.6.2"
  },
  "dependencies": {
    "concurrently": "^7.2.1",
    "json-server": "^0.17.0",
    "react": "^17.0.2",
