{
  "name": "dirtybird",
  "version": "1.0.0",
  "description": "dirty bird",
  "main": "index.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "lint": "eslint \"src/**/*.{js,jsx}\" --quiet",
    "start": "parcel src/index.html",
    "server": "json-server --watch db.json --port 3001",
